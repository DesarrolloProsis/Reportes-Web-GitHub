@model ReportesWeb1_2.ModelsReportes.CajeroReceptorModel

@if (Model.ListBolsas != null && Model.ListBolsas.Any())
{
    <div class="col-lg-12">
        <p class="text-center lead">LISTA DE BOLSAS</p>
        <table class="table table-striped" id="mytable">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Inicio</th>
                    <th scope="col">Fin</th>
                    <th scope="col">Carril</th>
                    <th scope="col">Bolsa</th>
                    <th scope="col">Acciones</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.ListBolsas)
                {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <td>@item.Inicio</td>
                        <td>@item.Fin</td>
                        <td>@item.Carril</td>
                        <td>@item.Bolsa</td>
                        <td><a class="btn btn-primary" href="@Url.Action("ReportCajeroReceptorView", "Reportes", new { IdRow = item.Id})" target="_blank" @*onclick="InLoaderVal()"*@ id="loaderButton" role="button">Generar</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
else
{
    <div class="col-lg-12">
        <p class="text-center lead">NO HAY DATOS</p>
    </div>
}
@if (ViewBag.Error != null && ViewBag.Error != "")
{
    <div class="alert alert-danger col-md-12 text-center" role="alert">
        @ViewBag.Error
    </div>
}
